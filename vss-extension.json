{
    "manifestVersion": 1,
    "id": "stryker-mutation-test",
    "publisher": "RafaelNascimento",
    "version": "0.1.0",
    "name": "Stryker Mutation Test Solution",
    "description": "Allows you to run stryker mutation tests for a .NET solution in azure pipelines",
    "public": true,
    "links": {
        "home": { "uri": "https://github.com/JRafaelNascimento/stryker-vss-ext#readme" },
        "repository": { "uri": "https://github.com/JRafaelNascimento/stryker-vss-ext" },
        "support": { "uri": "https://github.com/JRafaelNascimento/stryker-vss-ext/issues" },
        "getstarted": { "uri": "https://github.com/JRafaelNascimento/stryker-vss-ext#readme" },
        "privacypolicy": { "uri" : "https://gist.github.com/ThiagoBarradas/eb56552b4a71cf52141b85a6afa0e526" }
    },
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "test",
        "tests",
        "mutation",
        "stryker",
        "build"
    ],
    "icons": {
        "default": "images/extension-icon.png"
    },
    "targets": [
        { "id": "Microsoft.VisualStudio.Services" }
    ],
    "content": {
        "details": { "path": "README.md" },
        "license": { "path": "LICENSE.md" }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/JRafaelNascimento/stryker-vss-ext"
    },
    "contributions": [
        {
            "id": "stryker-solution-test-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": { "name": "task" }
        }
    ],
    "files": [
        { 
            "path": "src/strykersolutiontest" 
        },
        { 
            "path": "images", 
            "addressable": true 
        },
        { 
            "path": "node_modules", 
            "packagePath": "task/node_modules" 
        }
    ]
}